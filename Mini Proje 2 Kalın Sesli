
		char harf = 'b';
		char[] harfler = {'a','e','ı','i','o','ö','u','ü'};
		boolean sonuc = false;
		
		for (int i = 0; i < harfler.length; i++) {
			if (harf == harfler[i]) {
				sonuc = true;
			}
		}
		if (sonuc == true) {
			System.out.println(harf + " Kalın sesli bir harf");
		} else {
			System.out.println(harf + " İnce sesli bir harf");

		}

	
